{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "moment.js@2.29.1",
      "destination": "wwwroot/lib/moment.js/"
    },
    {
      "provider": "cdnjs",
      "library": "malihu-custom-scrollbar-plugin@3.1.5",
      "destination": "wwwroot/lib/malihu-custom-scrollbar-plugin/",
      "files": [
        "jquery.mCustomScrollbar.concat.min.js",
        "mCSB_buttons.png",
        "jquery.mCustomScrollbar.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-confirm@3.3.4",
      "destination": "wwwroot/lib/jquery-confirm/",
      "files": [
        "jquery-confirm.min.js",
        "jquery-confirm.min.css"
      ]
    }
  ]
}